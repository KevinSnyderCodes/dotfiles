# Aliases -------------------

# preferred options
alias rm="rm -iv"         # always confirm deletion
alias mv="mv -iv"        # always confirm overwrite
alias cp="cp -iv"        # always confirm overwrite
alias mkdir="mkdir -pv"  # create parent directories if they don't exist
# short notation
alias c="clear"
alias e="vim"
alias g="git"
alias l="ls -lash"
# directory traversal
alias ..="cd .."
alias .1="cd .."
alias .2="cd ../.."
alias .3="cd ../../.."
alias .4="cd ../../../.."
alias .5="cd ../../../../.."
alias .6="cd ../../../../../.."
alias .7="cd ../../../../../../.."
alias .8="cd ../../../../../../../.."
alias .9="cd ../../../../../../../../.."
